#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int num;
    scanf("%d", &num);
    if(num%2==0){
    printf("%d", num/2);
    }
    else{
        printf("%d", num/2 + 1);
    }

    return 0;
}
